<!-- BEGIN newspost -->
<table border="0" cellspacing="1" cellpadding="0" width="100%">
  <tr>
    <td class="newsHeader" width="100%">
      <table cellspacing="0" cellpadding="0" width="100%">
        <tr>
          <td nowrap>Posted by <a href="{newspost.POSTER_PROFILE_URL}">{newspost.POSTER_NAME}</a> - {newspost.POST_DATE}</td>
          <td align="right">{newspost.EDIT_LINK}</td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td class="newsMessage" colspan="2" style="padding-bottom: 20px;">{newspost.MESSAGE}</td>
  </tr>
</table>
<!-- END newspost -->